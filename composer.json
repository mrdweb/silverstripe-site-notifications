{
    "name": "dynamic/silverstripe-site-notifications",
    "description": "Display site notifications like violators and pop ups",
    "type": "silverstripe-vendormodule",
    "authors": [
        {
            "name": "Dynamic",
            "email": "dev@dyamicagency.com",
            "homepage": "https://www.dynamicagency.com"
        }
    ],
    "keywords": [
        "silverstripe",
        "CMS"
    ],
    "license": "BSD-3-Clause",
    "require": {
        "dynamic/silverstripe-linkable": "*",
        "silverstripe/recipe-cms": "^4.0 || ^5.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^5.7",
        "squizlabs/php_codesniffer": "^3.0"
    },
    "autoload": {
        "psr-4": {
            "Dynamic\\SiteMessages\\": "src/",
            "Dynamic\\SiteMessages\\Test\\": "tests/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "2.x-dev"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
